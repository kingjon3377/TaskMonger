# See https://editorconfig.org for more details

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true

[*.java]
indent_style = tab
indent_size = 4

[*.html]
indent_style = space
indent_size = 2

[*.iml]
indent_style = space
indent_size = 2

[*.sh]
indent_style = tab
indent_size = 4
